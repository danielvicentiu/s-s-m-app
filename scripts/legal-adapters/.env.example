# Copy this file to .env and fill in your values
# .env is git-ignored — never commit real credentials

# Supabase project URL (from Supabase dashboard → Project Settings → API)
SUPABASE_URL=https://your-project-id.supabase.co

# Service role key (from Supabase dashboard → Project Settings → API → service_role)
# WARNING: This key bypasses Row Level Security. Keep it secret!
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# Set to "true" to preview output without writing to the database
DRY_RUN=true
